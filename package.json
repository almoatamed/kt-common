{
  "name": "kt-common",
  "version": "1.0.0",
  "description": "Common Utilities",
  "keywords": [
    "common",
    "kt",
    "katana"
  ],
  "author": "Salem Elmotamed s.elmotamed96@gmail.com",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/almoatamed/kt-common.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts --out-dir dist --sourcemap",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^6.7.0",
    "typescript": "^5.6.2",
    "@types/node": "^20.2.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "async-lock": "^1.0.0",
    "typescript": "^5",
    "axios": "^1.0.0"
  },
  "dependencies": {
    "@types/async-lock": "^1.4.2"
  }
}